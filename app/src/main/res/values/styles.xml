<resources>
    <!-- Base application theme. -->
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimaryDark">#152238</item>

        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBarBlack">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimaryDark">#000000</item>

        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#18415C</item>

        <item name="colorPrimaryDark">#152238</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>
    <style name="AppTheme2" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->


        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">#152238</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="MyProgressBarRectangle" parent="@style/Widget.AppCompat.ProgressBar">
        <item name="android:indeterminateDrawable">@drawable/progressbar</item>
        <item name="android:minHeight">80dp</item>
        <item name="android:minWidth">200dp</item>
    </style>
    <style name="AlertDialogCustom" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/text</item>
        <item name="android:textColorPrimary">#000</item>
        <item name="android:background">@color/colorPrimaryDark</item>
    </style>


</resources>